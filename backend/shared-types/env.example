# Local development settings for Shared Types Service
DOCKER_ENV=false

# Database (local uses crm_campaigns_db)
DATABASE_URL=postgresql+asyncpg://postgres:admin@localhost:5432/crm_campaigns_db

# Redis
REDIS_URL=redis://localhost:6379

# Service settings
SERVICE_PORT=8006
SERVICE_NAME=shared-types

# Security
SECRET_KEY=super-secret-key-for-development-change-in-production-minimum-32-chars
JWT_SECRET_KEY=jwt-secret-key-for-development-change-in-production-minimum-32-chars
JWT_REFRESH_SECRET_KEY=your-jwt-refresh-secret-key

# Other services (for local development)
USER_SERVICE_URL=http://localhost:8000
CAMPAIGN_SERVICE_URL=http://localhost:8002
PAYMENT_SERVICE_URL=http://localhost:8004
NOTIFICATION_SERVICE_URL=http://localhost:8000
ANALYTICS_SERVICE_URL=http://localhost:8003
INTEGRATION_SERVICE_URL=http://localhost:8005

# External services
TIKTOK_SHOP_API_KEY=your-tiktok-shop-api-key
TIKTOK_SHOP_API_SECRET=your-tiktok-shop-api-secret
TIKTOK_SHOP_APP_ID=your-tiktok-shop-app-id
TIKTOK_SHOP_SHOP_ID=your-tiktok-shop-shop-id

# Discord settings
DISCORD_BOT_TOKEN=your-discord-bot-token
DISCORD_CLIENT_ID=your-discord-client-id
DISCORD_CLIENT_SECRET=your-discord-client-secret
DISCORD_REDIRECT_URI=your-discord-redirect-uri

# Stripe settings
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# Fanbasis settings
FANBASIS_API_KEY=your-fanbasis-api-key
FANBASIS_API_URL=your-fanbasis-api-url
FANBASIS_API_SECRET=your-fanbasis-api-secret

# SendBlue settings
SENDBLUE_API_KEY=your-sendblue-api-key

# AWS settings (optional)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-s3-bucket-name

# Payment settings
MINIMUM_PAYOUT_AMOUNT=10.0
MAXIMUM_PAYOUT_AMOUNT=10000.0
PAYOUT_PROCESSING_DAYS=["monday","friday"]
PAYMENT_RETRY_ATTEMPTS=3

# Email/SMTP settings
SMTP_HOST=your-smtp-host
SMTP_PORT=587
SMTP_USERNAME=your-smtp-username
SMTP_PASSWORD=your-smtp-password
FROM_EMAIL=noreply@launchpaid.com

# Celery settings
CELERY_BROKER_URL=redis://localhost:6379/4
CELERY_RESULT_BACKEND=redis://localhost:6379/5

# Rate limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_HOUR=1000
RATE_LIMIT_PER_MINUTE=100

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8006"]

# Environment
DEBUG=true
ENVIRONMENT=development
LOG_LEVEL=INFO 